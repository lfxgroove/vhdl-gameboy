#Runs before every @test part is run
@prepare {
#bytes that should be existent in the beginning
  01 02 03 10 30 40 0e
  [0162] b3 b4 b5
}

#This is a comment
#Comment
@test {
  @check {
    [C000] ab cd
    [C100] ef 01
  }
}

@test {
  03 40 4e a3
  [0160] 10 11 12 13 14
  @check {
    [C000] ab cd ef 00 01
    [C009] a0
    [C00B] b0
  }
}
